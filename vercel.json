{
  "version": 2,
  "builds": [
    {
      "src": "login-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "loginapp/pom.xml",
      "use": "@vercel/java"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/loginapp/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/login-frontend/$1"
    }
  ],
  "env": {
    "SPRING_PROFILES_ACTIVE": "vercel"
  }
}
